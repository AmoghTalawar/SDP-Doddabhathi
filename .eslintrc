{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "off",
    "react-hooks/exhaustive-deps": "warn",
    "eqeqeq": "warn",
    "jsx-a11y/anchor-is-valid": "warn",
    "jsx-a11y/aria-roles": "off",
    "no-script-url": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}